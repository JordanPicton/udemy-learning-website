    "editor.fontSize": 15,
    "editor.fontFamily": "Comfortaa, Consolas, 'Courier New', monospace",
    "terminal.integrated.fontFamily": "Consolas, 'Courier New', monospace",
    "editor.tabCompletion": "on",
